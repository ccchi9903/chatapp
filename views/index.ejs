		<div class="row">

			<div class="twelve wide column ui container">
			
				<h1>Snopchat</h1>
				
				<p id="typing">It's silent...</p>
				
				<ul id="messages"></ul>
				
				<p></p>
				<p></p>
				
			</div>
			
			<div class="four wide column sidebar">
			
				<h3>Online: <span id="onlinenumber">0</span></h3>
				
				<ul id="onlinenames"></ul>
				
			</div>
		</div>
	</div>
</div>

<form action="">

	<div id="emojiKeypad">
	
		<p id="emojiKeypadButton" onclick="toggleEmojiKeypad()">ðŸ˜€</p>
